{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "routes": [
        { "route": "/about-us", "rewrite": "/about-us.html" },
        { "route": "/terms-and-conditions", "rewrite": "/terms-and-conditions.html" },
        { "route": "/privacy-policy", "rewrite": "/privacy-policy.html" },
        { "route": "/terms-and-conditions", "rewrite": "/terms-and-conditions.html" }
      ]
    }    
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/api/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 