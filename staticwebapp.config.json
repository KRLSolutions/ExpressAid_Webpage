{
  "routes": [
    {
      "route": "/terms-and-conditions",
      "rewrite": "/terms-and-conditions.html"
    },
    {
      "route": "/privacy-policy",
      "rewrite": "/privacy-policy.html"
    },
    { 
      "route": "/about-us",
      "rewrite": "/about-us.html"
    },
    {
      "route": "/how-it-works",
      "rewrite": "/#how-it-works"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/terms-and-conditions",
      "/privacy-policy",
      "/about-us",
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
